aws cloudformation create-stack   --stack-name apim-rds-stack   --template-body ./scripts-cluster/apim-rds-cf.yaml --parameters ParameterKey=pDbUser,ParameterValue=root ParameterKey=pDbPass,ParameterValue=password123
aws rds wait db-instance-available --db-instance-identifier "apim-profile-automation-rds"

aws cloudformation describe-stacks --stack-name apim-rds-stack --query 'Stacks[?StackName==`apim-rds-stack`][].Outputs[?OutputKey==`ApimDBJDBCConnectionString`].OutputValue' --output text


aws s3 cp ./barname.1.bin s3://fooname/barname.bin


aws cloudformation wait stack-delete-complete --stack-name apim-rds-stack